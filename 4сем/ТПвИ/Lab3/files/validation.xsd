<?xml version="1.0" encoding="utf-8"?>
<xs:schema attributeFormDefault="unqualified" 
            elementFormDefault="qualified" 
            targetNamespace="http://msiter.ru" 
            xmlns:xs="http://www.w3.org/2001/XMLSchema">
<xs:element name="Container">
    <xs:complexType>
    <xs:sequence>
        <xs:element name="TaxiList">
        <xs:complexType>
        <xs:sequence>
            <xs:element name="Taxi" minOccurs="1" maxOccurs="unbounded">
                <xs:complexType>
                <xs:sequence>
                    <xs:element name="NameCar" type="xs:string" />
                    <xs:element name="Fuel" type="xs:double" />
                    <xs:element name="Speed" type="xs:positiveInteger" />
                    <xs:element name="Type">
                        <xs:simpleType>
                            <xs:restriction base="xs:string">
                                <xs:enumeration value="carType.economy"/>
                                <xs:enumeration value="carType.comfort"/>
                                <xs:enumeration value="carType.business"/>
                                <xs:enumeration value="carType.luxury"/>
                            </xs:restriction>
                        </xs:simpleType>
                    </xs:element>
                </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
        </xs:complexType>
    </xs:element>
    </xs:sequence>
</xs:complexType>
</xs:element>

</xs:schema>
